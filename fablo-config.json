{
  "$schema": "https://github.com/hyperledger-labs/fablo/releases/download/2.2.0/schema.json",
  "global": {
    "fabricVersion": "2.3.0",
    "tls": false,
    "engine": "docker",
    "peerDevMode": true
  },
  "orgs": [
    {
      "organization": {
        "name": "Orderer",
        "domain": "orderer.example.com"
      },
      "orderers": [
        {
          "groupName": "group1",
          "type": "solo",
          "instances": 1
        }
      ],
      "tools": {}
    },
    {
      "organization": {
        "name": "Org1",
        "domain": "org1.example.com"
      },
      "peer": {
        "instances": 1,
        "db": "LevelDb"
      },
      "tools": {}
    }
  ],
  "channels": [
    {
      "name": "my-channel1",
      "orgs": [
        {
          "name": "Org1",
          "peers": [
            "peer0",
            "peer1"
          ]
        }
      ]
    }
  ],
  "chaincodes": [
    {
      "name": "chaincode1",
      "version": "0.0.1",
      "lang": "java",
      "channel": "my-channel1",
      "directory": "./samples/chaincodes/java-chaincode"
    }
  ]
}