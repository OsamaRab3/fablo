{
  "$schema": "https://github.com/hyperledger-labs/fablo/releases/download/2.2.0/schema.json",
  "global": {
    "fabricVersion": "2.3.2",
    "tls": false,
    "peerDevMode": true
  },
  "orgs": [
    {
      "organization": {
        "name": "Orderer",
        "mspName": "OrdererMSP",
        "domain": "root.com"
      },
      "orderers": [
        {
          "groupName": "group1",
          "prefix": "orderer",
          "type": "solo",
          "instances": 1
        }
      ]
    },
    {
      "organization": {
        "name": "Org1",
        "mspName": "Org1MSP",
        "domain": "org1.example.com"
      },
      "ca": {
        "prefix": "ca"
      },
      "peer": {
        "prefix": "peer",
        "instances": 1,
        "db": "LevelDb"
      }
    }
  ],
  "channels": [
    {
      "name": "mychannel",
      "orgs": [
        {
          "name": "Org1",
          "peers": [
            "peer0"
          ]
        }
      ]
    }
  ],
  "chaincodes": [
    {
      "name": "simple-asset",
      "version": "1.0",
      "lang": "java",
      "channel": "mychannel",
      "directory": "./samples/java-chaincode",
      "endorsement": "AND ('Org1MSP.member')",
      "init": "{\"Args\":[]}"
    }
  ]
} 